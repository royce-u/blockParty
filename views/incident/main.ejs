
<div class="container center">
    <div class="row">
        <div class="col s12"><h1>main incident page</h1></div>
        <div class="col s3">
            <% post.forEach(p => { %>
                <p><a href="post/<%=p.id%>"</a><%=p.headline%></a></p>
                <p><%=p.content%></p>
                <p>-<%=p.user.username%></p>
            <% }) %>
        </div>
        <div class="col s9">
            <h3>Map maybe goes here?</h3>
        </div>
    </div>
</div>