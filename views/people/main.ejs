<div class="container center">
    <div class="row">
        <div class="col s12">
            <h3>Travel around the world without leaving your neighborhood</h3>
            <h5>A page for the culturally curious</h5>
        </div>
    </div>
    <div class="row">
        <div class="col s9 feed">
            <% post.forEach(p => { %>
            <p><a href="post/<%=p.id%>"><%=p.headline%></a></p>
            <p><%=p.content%></p>
            <p>-<%=p.user.username%></p>
            <% }) %>
        </div>
        <div class="col s3">
                <p class="carousel-item grey darken-4">Ever walked past your neighbors door while they were cooking and
                    wondered what that
                    amazing smell was? Try Dish Swap - Trade Jook for Congee, Pelmeni for Shumai, Focaccia for Naan</p>
                <p class="carousel-item">Ever wanted to learn Flamenco? You never know. Your local barista may be an
                    expert.
                    [insert dancer graphic here]</p>
                <p class="carousel-item">Or maybe your learning another language and you just want someone to talk
                    to.....</p>
        </div>
    </div>
</div>





<script>
    document.addEventListener('DOMContentLoaded', function () {
        var elems5 = document.querySelectorAll('.carousel');
        var instances = M.Carousel.init(elems5, {});
    });
</script>