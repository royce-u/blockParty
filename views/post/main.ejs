<div class="container feed">
    <div class="row">
        <div class="col s8">
            <% post.forEach(p => { %>
                <div class="feed"><a href="/post/<%=p.id%>"><%= p.headline %></a>
                    <p><%= p.content %></p>
                </div>
                    <% }) %>
        </div>
    </div>
</div>

<style>
    body{
        background-image: url("/css/images/newspaper.jpeg");
        background-size: cover;
    }
</style>


