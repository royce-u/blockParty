<div class="row">
    <div class="col s12 m6">
        <% post.forEach(p => { %>
        <div class="card blue-grey darken-1">
            <div class="card-title"><a href="/post/<%=p.id%>"><%= p.title %></a></div>
                <p><%= p.content %></p>
                <p><%= p.zipcode %></p>
                <% }) %>
        </div>
    </div>
</div>